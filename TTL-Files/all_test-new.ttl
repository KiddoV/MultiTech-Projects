sendln 'at+gmm'
waitln 'LE866A1-JS'
if result = 0 then
    messagebox 'INCORRECT TELIT MODULE. EXPECTED: LE866A1-JS' 'MODULE FAILURE'
   	disconnect
   	closett
   	end
endif
flushrecv
mpause 500
sendln 'at+gmr'
waitln '23.00.204'
if result = 0 then
    messagebox 'INCORRECT FIRMWARE VERSION. EXPECTED: 23.00.204' 'FIRMWARE FAILURE'
	disconnect
   	closett
   	end
endif
mpause 500 
flushrecv
sendln 'at#cfvr'
waitln '8'
if result=0 then
    messagebox 'INCORRECT TELIT FIRMWARE BUILD. EXPECTED: 8' 'FIRMWARE BUILD FAILURE'
	disconnect
   	closett
   	end
endif

statusbox 'SMC TEST PASSED' 'PASSED'
mpause 500
send 24 ;Exit 'at' command promt
closesbox
goto SHORTING_MTCDT

