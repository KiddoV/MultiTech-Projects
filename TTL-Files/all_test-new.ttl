:csq_check
mpause 500
sendln 'AT+CSQ'
wait '99'
if result =1 goto csq_check
mpause 500
sendln 'at+csq'
wait 'OK'
mpause 500
sendln 'at+csq'
wait 'OK'
timeout = 30
mpause 500
sendln 'at+csq'
waitrecv '+CSQ: ' 10 1
strconcat rssi3 inputstr
strremove rssi3 1 6
strremove rssi3 3 2
str2int strvar rssi3
strtrim rssi3 ','
statusbox rssi3 'RSSI'
if strvar >= 7 then
    mpause 1000
else
    beep
    messagebox 'LOW RSSI. CSQ SIGNAL STRENGTH FAILURE!' 'CSQ CHECK FAILURE'
    disconnect
    closett
    end
endif

flushrecv
mpause 500
sendln 'at#moni'
waitln '#MONI:'
	   if result=0 goto NETWORK_CONNECTION_FAILURE
strscan inputstr  'PWR:'
result= result+4
strcopy inputstr result 4 sig_level
strtrim sig_level 'd'
statusbox  sig_level 'Signal Level'
str2int rssi2 sig_level
if rssi2 < -99 goto LOW_RSSI
mpause 1000
closesbox
mpause 500
flushrecv
sendln 'at#servinfo'
waitln 'T-Mobile' 'AT&T' 'Verizon'
	if result=0 goto NETWORK_CONNECTION_FAILURE
pause 1
sendln 'at#moni'
waitln 'T-Mobile' 'AT&T' 'Verizon'
	if result=0 goto NETWORK_CONNECTION_FAILURE
pause 1

statusbox 'SMC TEST PASSED' 'PASSED'
mpause 500
send 24 ;Exit 'at' command promt
closesbox
goto SHORTING_MTCDT

