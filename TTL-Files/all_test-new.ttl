:SHORTING_MTCDT
if is240L=0 goto RESULT_LOGGING
if is246L=0 goto GPS_TEST
if is247L=0 goto GPS_TEST
if is210L=0 goto RESULT_LOGGING

:GPS_TEST
timeout = 5
mpause 500
flushrecv
sendln 'gpspipe -r'
wait '$GPGGA' '$GPZDA' '$GPRMC' '$GPGSA' '$GNGLL' '$GPGSV' '$GNGSA'
if result = 0 then
    try = try + 1
    if try < 2 then
   	   send 3
	   wait '#'
	   sendln '/etc/init.d/gpsd stop'
	   wait '#'
	   sendln '/etc/init.d/gpsd start'
	   wait 'pps pps1: source "/dev/ttyXRUSB2" added' '#'
	   send 3
	   wait '#'
	   goto GPS_TEST
	endif
	try = 0
	goto gps_faliure
elseif result > 0 then
	 timeout = 80
	 wait '4505.' '4506.' 'could not connect'
	 if result=0 || result=3 then
	  	:gps_faliure
   	 	messagebox 'UNABLE TO GET GPS LOCATION' 'GPS TEST FAILURE'
   		disconnect
   		closett
   		end
	 endif
endif
send 3
wait '#'
pause 1
statusbox 'GPS TEST PASSED' 'PASSED'
mpause 500
closesbox
pause 1

if is247L != 0 goto RESULT_LOGGING

:WIFI_BT_TEST
timeout = 15
if isNONE = 0 then
    imei = 'NO'
endif
wifiAddr = ''
bluetoothAddr = ''

:start_wifi
mpause 500
sendln '/etc/init.d/rs9113 stop >/dev/null 2>&1'
wait '#'
mpause 500
sendln 'export COEX_MODE=5'
wait '#'
mpause 500
sendln 'cd /opt/rs9113;bash /opt/rs9113/wlan_enable.sh >/dev/null 2>&1'
wait '#' 'No such file or directory'
if result=2 goto WIFI_FAILURE
mpause 500
sendln '/opt/rs9113/onebox_util rpine0 create_vap wifi1 sta sw_bmiss'
wait '#' 'No such file or directory'
if result=2 then
    timeout=10
	sendln 03
    mpause 500
    sendln '/etc/init.d/rs9113 stop >/dev/null 2>&1'
    waitln '#'
	sendln "sed -i 's/COEX_MODE=[0-9]/COEX_MODE=5/' /etc/default/rs9113"
	waitln '#'
	sendln '/etc/init.d/rs9113 start'
	waitln '#'
	sendln '/opt/rs9113/onebox_util rpine0 create_vap wifi1 sta sw_bmiss'
	flushrecv
	waitln 'cat' 'No such file or directory'
	if result=2 then
	   sendln 03
	   goto WIFI_FAILURE
	endif
endif
mpause 500
sendln 'ip link set dev wifi1 up'
wait 'Cannot find device' '#'
if result=1 goto WIFI_FAILURE
:wifi_addr_check
flushrecv
mpause 500
sendln "ifconfig |grep -i wifi1 |awk '{print $5}'"
waitln '00:' '88:' '80:'
strlen inputstr
if result <>17 then
   sendln 03
   goto WIFI_FAILURE
endif
strconcat wifiAddr inputstr

timeout=30
:wifi_scan
mpause 500
sendln 'iw dev wifi1 scan |grep -i SSID'
waitln  'Dummy' 'MTS-MOBILE' 'MTS-PRINTERS' 'MTS-MFG' 'MTS-IT'
if result > 0 then
    statusbox 'WIFI PASSED' 'PASSED1'
elseif result = 0 then
	goto WIFI_FAILURE
endif
mpause 1000
closesbox
