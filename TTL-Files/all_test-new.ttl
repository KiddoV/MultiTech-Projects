filewrite fhandle ",,"
filewrite fhandle itemNum
filewrite fhandle ",,"
filewrite fhandle productId
filewrite fhandle ",,"
filewrite fhandle tempstr
filewrite fhandle ",,"
filewrite fhandle imei
filewrite fhandle ",,"
filewrite fhandle radioVersion
filewrite fhandle ",,"
filewrite fhandle telModuleVer
filewrite fhandle ",,"
