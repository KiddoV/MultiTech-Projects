mpause 500
sendln 'at+cpin?'
wait 'OK' 'not found' 'ERROR'
simResult = result
:check_live_sim
if isH5 = 0 then
    :registration
	mpause 500
    sendln 'at+cgdcont=1,"IP","fast.t-mobile.com"'
    wait 'OK'
    mpause 500
    sendln 'at+creg?'
	waitln '+CREG: 0,1' '+CREG: 0,2'
	:connection
    mpause 500
    sendln 'at#sgact=1,1'
    wait '#SGACT:' 'ERROR'
    if result = 2 then
        messagebox 'THIS SMC NEED A LIVE SIM. PLEASE ADD!' 'LIVE SIM FAILURE'
        disconnect
  		closett
  		end
	endif
	sendln 'at#sgact=1,0'
	wait 'OK'
endif

if simResult=1 then
    statusbox 'SIM CHECK PASSED' 'PASSED'
	mpause 1000
	closesbox
else
  beep
  messagebox 'SIM TEST FAILURE!' 'SIM FAILURE'
  disconnect
  closett
  end
endif
mpause 1000
send 24 ;Exit 'at' command promt

