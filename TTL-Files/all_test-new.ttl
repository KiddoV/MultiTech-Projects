:SMC_TEST
imei = ''
timeout = 3

mpause 500
wait '#'
sendln 'microcom /dev/modem_at0'
:init
mpause 500
sendln 'at'
waitln 'OK'
if result=0 goto init
mpause 500
sendln 'ate1'
wait 'OK'
mpause 500
sendln 'at+cgsn'
waitln 'ERROR' '35' '0'
if result = 1 then
    messagebox 'COULD NOT FIND IMEI. PLEASE REPORT!' 'IMEI CHECK FAILURE'
    disconnect
   	closett
   	end
endif
strconcat imei inputstr
mpause 500
sendln 'at+cgsn'
wait 'OK'
mpause 500
sendln 'at#gpio=1,0,2'
waitln'OK'
mpause 500
sendln 'at#sled=2'
waitln 'OK'
mpause 500
sendln 'at#sledsav'
wait 'OK'

:temp_setting
flushrecv
mpause 500
sendln 'at#tempcfg?'
if isL4E1=0 then
    waitln '#TEMPCFG: -273,-33,0,-35,-28,0,-30,97,0,95,102,2,100,528,3'
elseif isH5=0 then
	waitln '#TEMPCFG: -30,-10,55,80'
elseif isL4N1=0 then
	waitln '#TEMPCFG: -273,-33,0,-35,-28,0,-30,94,0,80,99,2,82,105,3'
elseif isLAT3=0 then
	waitln '#TEMPCFG: -40,-15,55,85'
elseif isLAT1=0 then
	waitln '#TEMPCFG: -40,-33,0,-35,-28,0,-30,89,0,79,95,3,89,100,3'
elseif isLVW2=0 then
	waitln '#TEMPCFG: -273,-33,0,-35,-28,0,-30,95,0,93,100,3,98,528,3'
elseif isLSB3=0 then
	waitln '#TEMPCFG: -40,-15,55,85'
	if result = 0 then
	    sendln '#TEMPCFG: -40,-15,55,85'
		goto temp_setting
	endif
endif
if result=0 goto temp_setting
mpause 500
sendln 'at#rxdiv?'
if isLat3=0 then
    wait '#RXDIV: 1,3'
elseif isH5=0 then
 	wait '#RXDIV: 0,1'
elseif isLVW2=0 then
 	wait '#rxdiv: 1,1'
elseif isLSB3=0 then
 	wait '#RXDIV: 1,0'
else
 	wait '#RXDIV: 1,1'
endif
if result = 0 then
    mpause 500
    sendln 'at#rxdiv=1,1'
endif

:apn1_check
flushrecv
mpause 500
if isLVW2=0 then
    sendln 'at+cgdcont?'
	waitln '+CGDCONT: 1,"IPV4V6","vzwims"'
	if result =0 then
	sendln 'at+cgdcont=1,"IPV4V6","vzwims"'
	wait 'OK'
	endif
	mpause 1000
	sendln 'at+cgdcont?'
	waitln '+CGDCONT: 2,"IPV4V6","vzwadmin"'
	if result =0 then
	sendln 'at+cgdcont=2,"IPV4V6","vzwadmin"'
	wait 'OK'
	endif
	mpause 1000
	sendln 'at+cgdcont?'
	waitln '+CGDCONT: 3,"IPV4V6","vzwinternet"'
	if result =0 then
	sendln 'at+cgdcont=3,"IPV4V6","vzwinternet"'
	wait 'OK'
	endif
	goto LVW2_TEST
endif

sendln 'at+cgdcont?'
if isH5=0 then
    wait 'OK'
    mpause 500
    sendln 'at+cgdcont=1'
    wait 'OK'
    goto H5_TEST
endif
waitln '+CGDCONT: 1,"IPV4V6",""'
if result =0 then
    sendln 'at+cgdcont=1,"IPV4V6","",""'
    goto apn1_check
endif

if isLAT3=0 goto SHORTING_SMC
if isLSB3=0 goto SHORTING_SMC

mpause 500
sendln 'at+cgdcont?'
waitln '+CGDCONT: 2,"IPV4V6","ims"'
if result =0 then
    sendln 'at+cgdcont=2,"IPV4V6","ims"'
    goto apn1_check
endif
mpause 500
sendln 'at+cgdcont?'
waitln '+CGDCONT: 3,"IPV4V6","sos"'
if result =0 then
    sendln 'at+cgdcont=3,"IPV4V6","sos"'
    goto apn1_check
endif


:SHORTING_SMC
