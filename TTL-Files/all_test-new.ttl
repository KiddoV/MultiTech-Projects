;
;
connect '192.168.2.1 /ssh /auth=password /user=mtadm /passwd=root /nosecuritywarning'

timeout = 10
clearscreen 1
show 0
is240L = -1
is246L = -1
is247L = -1
radioVersion = 'N/A'
imei = 'N/A'
telModuleVer = 'N/A'
fwBuild = 'N/A'
