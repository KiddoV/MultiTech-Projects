:start_bt
send 13
wait '#'
mpause 500
sendln '/etc/init.d/rs9113 start >/dev/null 2>&1'
wait '#'
mpause 500
sendln 'hciconfig hci0 up'
wait 'No such device' '#'
if result=1 goto BT_FAILURE

:bt_addr_check
flushrecv
mpause 500
sendln "hcitool dev |grep -i hci0 |awk '{print $2}'"
waitln '00:' '88:' '80:'
if result=0 goto BT_FAILURE
strlen inputstr
if result <>17 goto start_bt
strconcat bluetoothAddr inputstr

timeout=50
:bt_scan
mpause 500
sendln "hcitool scan"
waitln 'Windows CE' 'MUSKYS_blue' 'slave' 'G5' 'WindowsCE' 'OPERATOR-PC' 'ADMIN-PC' 'MTHS' 'TESTROOM' 'TESTROOM9'
if result > 0 then
    statusbox 'BLUETOOTH PASS' 'PASSED2'
elseif result = 0 then
	goto BT_FAILURE
endif
mpause 1000
closesbox
send 13

